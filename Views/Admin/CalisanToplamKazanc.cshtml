@{
    ViewData["Title"] = "Günlük Bütçe";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class ="table table-striped">
    <thead>
        <tr>
            <th>Çalışan Adı</th>
            <th>Toplam Kazanç</th>
            <th>Toplam Randevu Sayısı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.CalisanToplamKazanc)
        {
            <tr>
                <td>@item.CalisanAdi</td>
                <td>@item.ToplamKazanc</td>
                <td>@item.ToplamRandevuSayisi</td>
            </tr>
        }
    </tbody>
</table>



<!-- Bootstrap 5 CDN bağlantıları -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" />
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>